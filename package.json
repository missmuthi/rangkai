{
  "name": "rangkai",
  "private": true,
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "preview": "npx nuxthub preview",
    "deploy": "npm run build && npx wrangler pages deploy dist --project-name rangkai",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "npx standard-version",
    "typecheck": "nuxt typecheck",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "pnpm test && pnpm test:e2e",
    "record:demo": "node scripts/record-scanner.mjs",
    "migrate:prod": "npx wrangler d1 execute rangkai-db --remote --file=./server/db/migrations/0001_init.sql"
  },
  "nuxthub": {
    "hooks": {
      "deploy": "npm run migrate:prod"
    }
  },
  "dependencies": {
    "@nuxt/eslint": "^1.4.1",
    "@nuxt/image": "^2.0.0",
    "@nuxthub/core": "^0.9.0",
    "@nuxtjs/color-mode": "^4.0.0",
    "@nuxtjs/security": "^0.0.0",
    "@types/uuid": "^10.0.0",
    "better-auth": "^1.4.5",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.7",
    "fast-xml-parser": "^5.3.2",
    "lucide-vue-next": "^0.556.0",
    "nuxt": "^3.17.5",
    "nuxt-security": "^2.5.0",
    "tailwind-merge": "^3.4.0",
    "uuid": "^13.0.0",
    "vue-qrcode-reader": "^5.7.3",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^1.4.1",
    "@nuxt/test-utils": "^3.20.1",
    "@nuxt/ui": "^2.22.3",
    "@playwright/test": "^1.57.0",
    "@types/node": "^20.11.4",
    "@vite-pwa/nuxt": "^1.1.0",
    "@vitest/coverage-v8": "^4.0.15",
    "@vue/test-utils": "^2.4.6",
    "@vueuse/core": "^14.1.0",
    "drizzle-kit": "^0.31.7",
    "eslint": "^9.29.0",
    "happy-dom": "^20.0.11",
    "sharp": "^0.34.5",
    "standard-version": "^9.5.0",
    "tailwindcss": "3.4.15",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15",
    "vue-tsc": "^2.2.10",
    "wrangler": "^4.52.1"
  },
  "packageManager": "pnpm@10.12.1",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild",
      "sharp",
      "unrs-resolver",
      "workerd"
    ],
    "overrides": {
      "ms": "3.0.0-beta.2"
    },
    "patchedDependencies": {
      "ms": "patches/ms.patch"
    }
  }
}
